{% extends "base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets
        'css/style-default.css'
        filter='cssrewrite' output='css/compiled/app.default.css' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body %}
{% include "components/navbar_default.html.twig" %}
